
public class TestClient {
	public static void main(String[] args) {
		Account accountMarcela = new Account();
		Client clientMarcela = new Client();
		clientMarcela.name = "Marcela";
		
		accountMarcela.customerName = clientMarcela;
		
		System.out.println(accountMarcela.customerName);
		System.out.println(clientMarcela.name);
		System.out.println(accountMarcela.customerName.name);
		
		Account accountPaulo = new Account();
		accountPaulo.customerName = new Client();
		accountPaulo.customerName.name = "Paulo";
		System.out.println(accountPaulo.customerName.name);

	}

}
